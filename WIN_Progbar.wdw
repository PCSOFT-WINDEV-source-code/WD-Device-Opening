   -
     name : AddProgram
     internal_properties : 
     procedure_id : 1635367608961398404
     type_code : 14
     code : |1-
      // Summary: Callback automatically called when listing the files
      // Syntax:
      //AddProgramm (<Path>, <Name>, <Change>, <Pointer>)
      //
      // Parameters:
      // 	Path: <specify the role of Path>
      // 	Name: <specify the role of Name>
      // 	Change: <specify the role of Change>
      // 	Pointer: <specify the role of Pointer>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      PROCEDURE AddProgram(Path, Name, Change, Pointer)
      
      // Variable
      clIcon is CAssociatedIcon
      
      // Path of the program
      STC_ProgramPath = Path+Name
      
      // Retrieve the associated icon
      IF clIcon:GetIcon(Path+Name,IMG_Memory..Name) THEN
      	// Insert into the looper
      	LooperAddLine(WIN_Main.LOOP_Application,dSaveImagePNG(IMG_Memory,inMemory),fExtractPath(Path+Name,fFileName),Path+Name)
      	
      	dEndDrawing(IMG_Memory)
      END
     type : 458752
