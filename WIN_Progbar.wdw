      CONSTANT
      	FILTER_Exe = "*.exe"
      END
   -
     code : |1-
      
      // Fill the array for the list of applications
      sPath   is string
      
      // Initialization
      sPath = SysDir(srProgramFiles) + ["\"] + FILTER_Exe
      
      // Load the executable files
      fListFile(sPath, "AddProgram",frRecursive+fdInterruptible)
      
      // Sort in ascending alphabetical order and select the first application
      LooperSort("+WIN_Main.LOOP_Application.ATT_Application")
      LooperSelectPlus(WIN_Main.LOOP_Application,1)
      ExecuteProcess(WIN_Main.LOOP_Application,trtSelection)
      
      Close(WIN_Progbar)
